typeListText":["medium armor"],"_typeHtml":"medium armor","_attunement":null,"_attunementCategory":"No Attunement Required","_fProperties":[],"_fMisc":["SRD","Mundane"],"_fValue":50,"_fTier":["none"],"_fIsMundane":true,"_fBonus":[],"_vType":"Medium armor"}}}},"img":"systems/dnd5e/icons/items/armor/scale.png","effects":[{"_id":"ez0eowYCULVgDO0a","flags":{},"changes":[{"key":"data.attributes.ac.value","value":"13 + {2, @abilities.dex.mod}kl","mode":4,"priority":4}],"disabled":false,"duration":{"startTime":null,"seconds":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"icon":"systems/dnd5e/icons/items/armor/scale.png","label":"Base AC","tint":"","transfer":true}],"_id":"dtb5CCdpZluPRlo3"}
